# https://coverage.readthedocs.io/en/latest/excluding.html
[run]
# Use relative paths for codecov compatibility
relative_files = True

[report]
exclude_also =
    def __repr__
    if self.debug:
    if settings.DEBUG
    raise AssertionError
    raise NotImplementedError
    if __name__ ?== ?.__main__.:
    if TYPE_CHECKING:
    class .*\bProtocol\):
    @(abc\.)?abstractmethod

omit =
    config.py
    config-3.py
